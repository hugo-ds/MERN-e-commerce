  "bugs": {
    "url": "https://github.com/hugo-ds/MERN-eCommerce-From-Scratch/issues"
  },
  "homepage": "https://github.com/hugo-ds/MERN-eCommerce-From-Scratch#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.12.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  }
